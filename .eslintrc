{
  "parser": "babel-eslint",
  "plugins": [
    "jsdoc"
  ],
  "rules": {
    "jsdoc/check-param-names": 1,
    "jsdoc/check-tag-names": 1,
    "jsdoc/check-types": 1,
    "jsdoc/require-param": 2,
    "semi": [
      2,
      "always"
    ],
    "indent": [
      2,
      2
    ],
    "linebreak-style": [
      2,
      "unix"
    ]
  }
}